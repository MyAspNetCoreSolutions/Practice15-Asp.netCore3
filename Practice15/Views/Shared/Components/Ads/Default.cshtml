@model  List<Practice15.Models.Ads>
@{
    ViewData["Title"] = "defaults";
}
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-3">
                    <div class="card">
                        <img class="card-img-top" src="@item.ImageName" alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title">@item.Title</h5>
                            <p class="card-text">@item.Description</p>
                            <a href="#" class="btn btn-primary">Go somewhere</a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
